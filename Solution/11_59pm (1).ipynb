        "    elif val > 19 and val < 25:\n",
        "        #val = 'от 18 до 24'\n",
        "        val = 2\n",
        "    elif val > 25 and val < 35:\n",
        "        #val = 'от 25 до 34'\n",
        "        val = 3\n",
        "    elif val > 35 and val < 55:\n",
        "        #val = 'от 35 до 55'\n",
